{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0b0c0c90",
                "editor.background": "#0b0c0c90",
                "editor.foreground": "#c2c2c2",
                "text": "#c2c2c2",
                "text.muted": "#c2c2c270",
                "text.ignored": "#c2c2c240",
                "text.placeholder": "#c2c2c250",
                "ignored": "#c2c2c230",
                "element.hover": "#c2c2c230",
                "ghost_element.hover": "#5A3C8530",
                "ghost_element.selected": "#5A3C8530",
                "ghost_element.active": "#5A3C8560",
                "border": "#0b0c0c",
                "editor.highlighted_line.background": "#5A3C8510",
                "editor.active_line.background": "#5A3C8510",
                "panel.background": "#0b0c0c90",
                "title_bar.background": "#0b0c0c90",
                "title_bar.inactive_background": "#0b0c0c90",
                "status_bar.background": "#0b0c0c90",
                "drop_target.background": "#0b0c0c90",
                "elevated_surface.background": "#0b0c0c",
                "toolbar.background": "#0b0c0c90",
                "tab_bar.background": "#0b0c0c90",
                "tab.inactive_background": "#0b0c0c90",
                "tab.active_background": "#5A3C8530",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0b0c0c",
                "scrollbar.thumb.background": "#5A3C85",
                "editor.gutter.background": "#0b0c0c90",
                "terminal.background": "#0b0c0c10",
                "terminal.foreground": "#c2c2c2",
                "terminal.dim_foreground": "#c2c2c2",
                "terminal.bright_foreground": "#c2c2c2",
                "terminal.ansi.black": "#0b0c0c",
                "terminal.ansi.red": "#5A3C85",
                "terminal.ansi.green": "#72648E",
                "terminal.ansi.yellow": "#8F7389",
                "terminal.ansi.blue": "#9571A4",
                "terminal.ansi.magenta": "#9F7CC3",
                "terminal.ansi.cyan": "#80899E",
                "terminal.ansi.white": "#c2c2c2",
                "terminal.ansi.bright_black": "#566a6a",
                "terminal.ansi.bright_red": "#5A3C85",
                "terminal.ansi.bright_green": "#72648E",
                "terminal.ansi.bright_yellow": "#8F7389",
                "terminal.ansi.bright_blue": "#9571A4",
                "terminal.ansi.bright_magenta": "#9F7CC3",
                "terminal.ansi.bright_cyan": "#80899E",
                "terminal.ansi.bright_white": "#c2c2c2",
                "modified": "#5A3C85",
                "syntax": {
                    "attribute": {
                        "color": "#5A3C85"
                    },
                    "boolean": {
                      "color": "#72648E"
                    },
                    "comment": {
                      "color": "#566a6a"
                    },
                    "comment.doc": {
                      "color": "#566a6a"
                    },
                    "constant": {
                      "color": "#72648E"
                    },
                    "function": {
                      "color": "#80899E"
                    },
                    "keyword": {
                      "color": "#9571A4"
                    },
                    "number": {
                      "color": "#9F7CC3"
                    },
                    "operator": {
                      "color": "#9571A4"
                    },
                    "string": {
                      "color": "#5A3C85"
                    },
                    "variable": {
                      "color": "#72648E"
                    }
                },
                "players": [
                  {
                    "cursor": "#5A3C85",
                    "background": "#0b0c0c",
                    "selection": "#5A3C8530"
                  }
                ]
            }
        }
    ]
}
